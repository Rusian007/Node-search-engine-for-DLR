  [1m[36mSection Create (0.3ms)[0m  [1m[32mINSERT INTO "sections" ("section_text", "rule_id", "created_at", "updated_at") VALUES (?, ?, ?, ?)[0m  [["section_text", "Section 467—Making a false document—The maker must dishonestly execute the document with the intention of causing it to be believed that such document was executed by or by the authority of a person by whom or by whose authority he knows that it was not executed. Al-haj Md. Serajuddowlah Vs. The State (Criminal) 198  "], ["rule_id", 25], ["created_at", "2023-03-22 13:07:25.200412"], ["updated_at", "2023-03-22 13:07:25.200412"]]
