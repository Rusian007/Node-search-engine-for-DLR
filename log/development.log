  [1m[36mSection Create (1.0ms)[0m  [1m[32mINSERT INTO "sections" ("section_text", "rule_id", "created_at", "updated_at") VALUES (?, ?, ?, ?)[0m  [["section_text", "Section 498—Sentence for one year—The Court ought to have exercised discretion in granting bail to the appellants in view of the short sentence of imprisonment. Saimuddin Vs. The State (Criminal)  151 -  "], ["rule_id", 10], ["created_at", "2023-03-22 13:04:38.177232"], ["updated_at", "2023-03-22 13:04:38.177232"]]
