  [1m[36mSection Create (0.4ms)[0m  [1m[32mINSERT INTO "sections" ("section_text", "rule_id", "created_at", "updated_at") VALUES (?, ?, ?, ?)[0m  [["section_text", "Section 137—Contention as to facts—The defence cannot make an ingenuous argument that the prosecution story cannot be believed as they did not mention about the connecting doors in between the rooms as the defence did not put any suggestion as to non-existence of the same. Abdul Quddus Vs. The State (Criminal) 234 "], ["rule_id", 17], ["created_at", "2023-03-22 13:05:39.942505"], ["updated_at", "2023-03-22 13:05:39.942505"]]
